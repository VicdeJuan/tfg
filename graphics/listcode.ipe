<text matrix="1 0 0 1 464.272 -320.692" transformations="translations" pos="272 752" stroke="black" type="label" width="564.257" height="406.765" depth="394.784" valign="baseline">\newcommand{\Elem}{\ensuremath{\mathit{Elem}}\xspace}

\begin{tabular}{c}
\begin{minipage}[c]{7cm}
\begin{algorithmic}[1]
	\algrestore{endAdd}
	\Statex $\ProcedureBegin \; \Remove (e)$
		\Statex {\quad \Taddr \; \prev}
		\Statex {\quad \Taddr \; \curr}
		\Statex {\quad \Taddr \; \aux}
	\Statex \tbf{begin}

	\State $\prev := \head$
	\State $\prev \pointsto \fLock()$
	\State $\curr := \prev \pointsto \fNext$
	\State $\curr \pointsto \fLock()$
	\While {$\curr \pointsto \fData &lt; e$}
		\State $\aux := \prev$
		\State $\prev := \curr$
		\State $\aux \pointsto \fUnlock()$
		\State $\curr := \curr \pointsto \fNext$
		\State $\curr \pointsto \fLock()$
	\EndWhile
	\If {$\curr \neq \tail \And \curr \pointsto \fData = e$}
		\State $\aux := \curr \pointsto \fNext$
		\State $\prev \pointsto \fNext := \aux$
		\Statex $\quad\;\; \region := \region \setminus \{curr\}$
		\Statex $\quad\;\; \elements := \elements \setminus \{e\}$
	\EndIf
	\State $\prev \pointsto \fUnlock()$
	\State $\curr \pointsto \fUnlock()$
	\State \tbf{return}
	\Statex $\ProcedureEnd$
\end{algorithmic}
\end{minipage}
\end{tabular}
</text>
