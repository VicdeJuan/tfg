<text matrix="1 0 0 1 -151.229 -336.23" transformations="translations" pos="272 752" stroke="black" type="label" width="564.257" height="422.302" depth="410.332" valign="baseline">\newcommand{\Elem}{\ensuremath{\mathit{Elem}}\xspace}

\begin{tabular}{c}
\begin{minipage}[c]{7cm}
\begin{algorithmic}[1]
	\algrestore{endSearch}
	\Statex $\ProcedureBegin \; \Insert (e)$
		\Statex {\quad \Taddr \; \prev}
		\Statex {\quad \Taddr \; \curr}
		\Statex {\quad \Taddr \; \aux}
	\Statex \tbf{begin}
	\State $\prev := \head$
	\State $\prev \pointsto \fLock()$
	\State $\curr := \prev \pointsto \fNext$
	\State $\curr \pointsto \fLock()$
	\While {$\curr \pointsto \fData &lt; e$}
		\State $\aux := \prev$
		\State $\prev := \curr$
		\State $\aux \pointsto \fUnlock()$
		\State $\curr := \curr \pointsto \fNext$
		\State $\curr \pointsto \fLock()$
	\EndWhile

	\If {$\curr \neq \fNull \And \curr \pointsto \fData &gt; e$}
		\State $\aux := \malloc(e, \fNull, \#)$
		\State $\aux \pointsto \fNext := \curr$
		\State $\prev \pointsto \fNext := \aux$
		\Statex $\quad\;\; \region := \region \cup \{\aux\}$
		\Statex $\quad\;\; \elements := \elements \cup \{e\}$
	\EndIf
	\State $\prev \pointsto \fUnlock()$
	\State $\curr \pointsto \fUnlock()$
	\State \tbf{return}
	\Statex $\ProcedureEnd$
	\algstore{endAdd}
\end{algorithmic}
\end{minipage}
\end{tabular}
</text>
