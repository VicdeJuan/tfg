<text matrix="1 0 0 1 -151.229 450.629" transformations="translations" pos="272 752" stroke="black" type="label" width="527.407" height="346.546" depth="334.412" valign="baseline">\newcommand{\Elem}{\ensuremath{\mathit{Elem}}\xspace}

\begin{tabular}{c}
\begin{minipage}[c]{6.5cm}
\begin{algorithmic}[1]
	\Statex \tbf{global}%
		\Statex $\quad \Taddr \; \head;
		\; \Taddr \; \tail;$
		\Statex $\quad \Tset \langle\Taddr\rangle \; \region;
		\; \Tset \langle\Telem\rangle \; \elements;$
	\Statex \tbf{assume}
		\Statex $ \hspace{-0.4em}
		\begin{array}{l&gt;{\hspace{-0.7em}}l}
			&amp; \region = \{\head, \tail, \fNull\} \\
			\And &amp; \elements = \{\head \pointsto \fData, \tail \pointsto \fData\} \\
			\And &amp; \head \neq \tail \And 
			\head \neq \fNull \And 
			\tail \neq \fNull \\
			\And &amp; \head \pointsto \fData = -\infty \And
			\tail \pointsto \fData = +\infty \\
			\And &amp; \head \pointsto \fNext = \tail \And 
			\tail \pointsto \fNext = \fNull
		\end{array}$

	\Statex $ $
	\Statex $\ProcedureBegin \; \MostGeneralClient$
		\Statex $\quad \Telem \; e$
	\Statex \tbf{begin}%
	\While {true}
	\State $e := \HavocListElem()$
	\State $\Nondet$
 	\State $\quad \Call \Search(e)$
	\Statex $\;\;\;\; \ProgOr$
	\State $\quad \Call \Insert(e)$
	\Statex $\;\;\;\; \ProgOr$
	\State $\quad \Call \Remove(e)$
%	\State $\left[
%		\begin{array}{l}
%		 	\Call \search(e) \\
%			\ProgOr \\
%			\Call \add(e) \\
%			\ProgOr \\
%			\Call \remove(e)
%		\end{array} \right]$
	\EndWhile
	\Statex $\ProcedureEnd$
	\algstore{endMGC}
\end{algorithmic}
\end{minipage}
\end{tabular}
</text>
<text matrix="1 0 0 1 464.272 452.498" transformations="translations" pos="272 752" stroke="black" type="label" width="564.257" height="344.677" depth="332.54" valign="baseline">\newcommand{\Elem}{\ensuremath{\mathit{Elem}}\xspace}

\begin{tabular}{c}
\begin{minipage}[c]{7cm}
\begin{algorithmic}[1]
	\algrestore{endMGC}
	\Statex $\ProcedureBegin \; \Search (e)$
		\Statex {\quad \Taddr \; \prev}
		\Statex {\quad \Taddr \; \curr}
		\Statex {\quad \Taddr \; \aux}
		\Statex {\quad \Tbool \; \found}
	\Statex \tbf{begin}
	\State $\prev := \head$
	\State $\prev \pointsto \fLock()$
	\State $\curr := \prev \pointsto \fNext$
	\State $\curr \pointsto \fLock$
	\While {$\curr \pointsto \fData &lt; e$}
		\State $\aux := \prev$
		\State $\prev := \curr$
		\State $\aux \pointsto \fUnlock()$
		\State $\curr := \curr \pointsto \fNext$
		\State $\curr \pointsto \fLock()$
	\EndWhile
	\State $\found := (\curr \pointsto \fData = e)$
	\State $\prev \pointsto \fUnlock()$
	\State $\curr \pointsto \fUnlock()$
	\State $\tbf{return}\; \found$
	\Statex $\ProcedureEnd$
	\algstore{endSearch}
\end{algorithmic}
\end{minipage}
\end{tabular}
</text>
